{
  "name": "@aerify/react",
  "version": "0.0.1",
  "description": "Aerify ",
  "keywords": [
    "boilerplate",
    "eslint",
    "library",
    "rollup",
    "typescript"
  ],
  "homepage": "https://github.com/blujedis/aerify#readme",
  "bugs": {
    "url": "https://github.com/blujedis/aerify/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blujedis/aerify.git"
  },
  "license": "MIT",
  "author": "Blujedi LLC <blujedicorp@gmail.com>",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "docs": "rimraf docs && typedoc src/index.ts && touch ./docs/.nojekyll",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "prebuild": "npm run clean",
    "pretest": "npm run lint",
    "build:js": "rollup -c",
    "build": "npm run build:js && npm run build:scss && npm run copy:scss",
    "prerelease": "npm run build",
    "postrelease": "git push --follow-tags",
    "release": "npx standard-version",
    "types:check": "tsc --noEmit",
    "types:watch": "npm run types:check -- --watch",
    "start": "rollup -c -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.48.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "fuse.js": "^6.4.6"
  }
}
